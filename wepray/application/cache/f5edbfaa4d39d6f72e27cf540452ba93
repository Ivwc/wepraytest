    var myUrl=window.location.href;
    if(myUrl.includes("temples_pic_")){
        document.getElementById('li_home').className = " ";
        document.getElementById('li_tpic_upload').className = "active";
        document.getElementById('li_store').className = " ";
        document.getElementById('li_money').className = " ";
    }else if(myUrl.includes("info")){
        document.getElementById('li_home').className = " ";
        document.getElementById('li_tpic_upload').className = " ";
        document.getElementById('li_store').className = " ";
        document.getElementById('li_money').className = " ";
